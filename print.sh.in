
print_usage() {
    cat <<HEREDOC
    usage: $(basename "${0}") <command>

        commands
            foo     do foo action.
            bar     do bar action.

            help    print this help and exit.
HEREDOC
}

print_info() {
    printf "[\033[1;35m+\033[0m] INFO: %s\n" "${1}"
}

print_warn() {
    printf "[\033[1;33m!\033[0m] \033[1;33mWARNING\033[0m: %s\n" "${1}"
}

print_error() {
    printf "[\033[1;34mx\033[0m] \033[1;34mERROR\033[0m: %s\n" "${1}"
    exit 1
}
